{% extends 'base.html.twig' %}

{% block title %}Hello RoomController!{% endblock %}

{% block main %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>


<h1>Room Details </h1>




<div class="form-horizontal"><!-- <form>  -->
    
    	<div class="form-group row">
    		<label class="col-sm-2 col-form-label">Id</label>
    		<div class="col-sm-10">
    		    <div class="form-control form-control-plaintext">{{ room.id }}</div>
    		</div>
    	</div>
    	
    	<div class="form-group row">
    		<label class="col-sm-2 col-form-label-lg">Address</label>
    		<div class="col-sm-10">
    		    <div class="form-control form-control-plaintext form-control-lg">{{ room.address }}</div>
    		</div>
    	</div>
    	
    	<div class="form-group row">
    		<label class="col-sm-2 col-form-label">Capacite</label>
    		<div class="col-sm-10">
    		    <div class="form-control form-control-plaintext form-control-lg">{{ room.capacity}}</div>
    		</div>
    		
    		<div class="form-group row">
    		<label class="col-sm-2 col-form-label">Price</label>
    		<div class="col-sm-10">
    		    <div class="form-control form-control-plaintext form-control-lg">{{ room.price}}</div>
    		</div>
    		
    	</div>
    	
    
    
    </div><!-- form-horizontal </form>  --> 
    
    <a href="{{ path('room_list') }}"><button class="btn btn-lg btn-primary">Back to list</button></a>
    






{% endblock %}
