{% extends 'base.html.twig' %}

{% block title %}Hello RoomController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>


<h1>Room index</h1>

<ul>
  {% for room in rooms %}
 
  
  <table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Address</th>
            <th>Capacity</th>
            <th> Owner </th>
            
          
        </tr>
    </thead>
    <tbody>

        <tr>
            <td>{{ room.id }}</td>
            <td>{{ room.address }}</td>
            <td>{{ room.capacity }}</td>
           <td>{{ owner}}</td>
            
            
        </tr>

    </tbody>
</table>
  
  
  
  {% endfor %} {# rooms #}
</ul>









{% endblock %}
