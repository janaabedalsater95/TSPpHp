{% extends 'base.html.twig' %}

{% block title %}Hello RoomController!{% endblock %}

{% block main %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>


<h1>Room index</h1>

<ul>

 
  
  <table class="table">
    <thead>
        <tr> 
       
            <th>Id</th>
            <th>Address</th>
            <th>Capacity</th>
           
            <th>Price</th>>
          
        </tr>
        
    </thead> 
    
    <tbody>
 {% for room in rooms %}
        <tr>
            <td>{{ room.id }}</td>
            <td>{{ room.address }}</td>
            <td>{{ room.capacity }}</td>
           
           <td>{{ room.price}}</td>
              <td><a href="{{ path('room_show', {'id': room.id}) }}">show</a></td>
            
        </tr>

    </tbody>{% endfor %} 
</table>
  
  
  
  
</ul>









{% endblock %}
